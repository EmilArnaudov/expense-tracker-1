
<h5>Showing Transactions from <input class="date-field" type="date" value="{{startingDate}}"> to <input class="date-field" type="date" value="{{endingDate}}"></h5>


<div class="table-wrapper">
<table class="table table-striped transaction-table">
    <thead>
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Type</th>
            <th scope="col">Budget</th>
            <th scope="col">Category</th>
            <th scope="col">Expense</th>
            <th scope="col">Amount</th>
        </tr>
    </thead>
  <tbody>
      {{#each allTransactions}}
        {{> transactionTableRow}}
      {{/each}}
  </tbody>
</table>


 